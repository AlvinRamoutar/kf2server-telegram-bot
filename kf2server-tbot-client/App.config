<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="kf2server_tbot_client.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="simpleAuthBinding">
          <security mode="TransportCredentialOnly">
            <transport clientCredentialType="Basic" />
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="simpleAuthBehavior">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
          <serviceCredentials>
            <userNameAuthentication userNamePasswordValidationMode="Custom" customUserNamePasswordValidatorType="TWCFClient.Auth.AuthManager, TWCFClient"/>
          </serviceCredentials>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="TWCFClient.Service.CurrentGameService" behaviorConfiguration="simpleAuthBehavior">
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="simpleAuthBinding" contract="TWCFClient.Service.ICurrentGameService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/TWCFClient.Service/CurrentGameService/" />
          </baseAddresses>
        </host>
      </service>
      <service name="TWCFClient.Service.AccessPolicyService" behaviorConfiguration="simpleAuthBehavior">
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="simpleAuthBinding" contract="TWCFClient.Service.IAccessPolicyService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/TWCFClient.Service/AccessPolicyService/" />
          </baseAddresses>
        </host>
      </service>
      <service name="TWCFClient.Service.SettingsService" behaviorConfiguration="simpleAuthBehavior">
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="simpleAuthBinding" contract="TWCFClient.Service.ISettingsService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/TWCFClient.Service/SettingsService/" />
          </baseAddresses>
        </host>
      </service>
      <service name="TWCFClient.Service.MiscellaneousService" behaviorConfiguration="simpleAuthBehavior">
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="simpleAuthBinding" contract="TWCFClient.Service.IMiscellaneousService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/TWCFClient.Service/MiscellaneousService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
    <userSettings>
        <kf2server_tbot_client.Properties.Settings>
            <setting name="KF2ServerURL" serializeAs="String">
                <value>http://kf2server.rhome.net:8080/ServerAdmin/</value>
            </setting>
            <setting name="WebAdminUsername" serializeAs="String">
                <value>admin</value>
            </setting>
            <setting name="WebAdminPassword" serializeAs="String">
                <value>WelcomeToBrampton69</value>
            </setting>
            <setting name="ServerInfoURL" serializeAs="String">
                <value>current/info</value>
            </setting>
            <setting name="ChangeMapURL" serializeAs="String">
                <value>current/change</value>
            </setting>
            <setting name="PlayersURL" serializeAs="String">
                <value>current/players</value>
            </setting>
            <setting name="PasswordsURL" serializeAs="String">
                <value>policy/passwords</value>
            </setting>
            <setting name="GeneralURL" serializeAs="String">
                <value>settings/general#SG_Game</value>
            </setting>
            <setting name="GameTypesURL" serializeAs="String">
                <value>settings/gametypes</value>
            </setting>
            <setting name="MapChangeTimeoutSeconds" serializeAs="String">
                <value>60</value>
            </setting>
            <setting name="DefaultTaskTimeoutSeconds" serializeAs="String">
