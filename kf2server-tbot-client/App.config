<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="kf2server_tbot_client.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.net>
    <defaultProxy>
    <proxy bypassonlocal="False" usesystemdefault="True" />
  </defaultProxy>
  </system.net>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="simpleAuthBinding">
          <security mode="TransportCredentialOnly">
            <transport clientCredentialType="Basic" />
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="simpleAuthBehavior">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
          <serviceCredentials>
            <userNameAuthentication userNamePasswordValidationMode="Custom"
              customUserNamePasswordValidatorType="kf2server_tbot_client.Security.AuthManager, kf2server-tbot-client, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
          </serviceCredentials>
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
		    <behavior name="simpleEB4HTTPClient">
		      <webHttp helpEnabled="true"/>
		    </behavior>
		  </endpointBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="simpleAuthBehavior" name="kf2server_tbot_client.Service.KF2Service">
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="simpleAuthBinding"
          contract="kf2server_tbot_client.Service.IKF2Service">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/kf2server_tbot_client.Service/KF2Service/" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
    <userSettings>
        <kf2server_tbot_client.Properties.Settings>
            <setting name="KF2ServerURL" serializeAs="String">
                <value>http://kf2server.rhome.net:8080/ServerAdmin/</value>
            </setting>
            <setting name="WebAdminUsername" serializeAs="String">
                <value>admin</value>
            </setting>
            <setting name="WebAdminPassword" serializeAs="String">
                <value>WelcomeToBrampton69</value>
            </setting>
            <setting name="ServerInfoURL" serializeAs="String">
                <value>current/info</value>
            </setting>
            <setting name="ChangeMapURL" serializeAs="String">
                <value>current/change</value>
            </setting>
            <setting name="PlayersURL" serializeAs="String">
                <value>current/players</value>
            </setting>
            <setting name="PasswordsURL" serializeAs="String">
                <value>policy/passwords</value>
            </setting>
            <setting name="GeneralURL" serializeAs="String">
                <value>settings/general#SG_Game</value>
            </setting>
            <setting name="GameTypesURL" serializeAs="String">
                <value>settings/gametypes</value>
            </setting>
            <setting name="MapChangeTimeoutSeconds" serializeAs="String">
                <value>90</value>
            </setting>
            <setting name="DefaultTaskTimeoutSeconds" serializeAs="String">
                <value>120</value>
            </setting>
            <setting name="UsersRelFilePath" serializeAs="String">
                <value>Users.dll</value>
            </setting>
            <setting name="PageNavTimeoutSeconds" serializeAs="String">
                <value>15</value>
            </setting>
            <setting name="Logfile" serializeAs="String">
                <value>Log.txt</value>
            </setting>
            <setting name="DefaultGamePassword" serializeAs="String">
                <value>there's no password like no password</value>
            </setting>
            <setting name="ServiceUsername" serializeAs="String">
                <value>1234</value>
            </setting>
            <setting name="ServicePassword" serializeAs="String">
                <value>5678</value>
            </setting>
            <setting name="ChatId" serializeAs="String">
                <value />
            </setting>
        </kf2server_tbot_client.Properties.Settings>
    </userSettings>
</configuration>